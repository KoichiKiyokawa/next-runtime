    "fetch-cookie": "^1.0.0",
    "form-data": "^3.0.1",
    "husky": "^4.3.8",
    "jest": "^27.2.0",
    "jest-cli": "^27.2.0",
    "jest-partial": "^1.0.1",
    "lint-staged": "^11.1.2",
    "next": "11.1.2",
