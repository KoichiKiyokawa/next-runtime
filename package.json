  "author": "Stephan Meijer <stephan@meijer.ws>",
  "version": "1.0.0",
  "private": false,
  "repository": "https://github.com/smeijer/next-runtime",
  "scripts": {
    "build": "rimraf ./lib *.tsbuildinfo && tsc",
